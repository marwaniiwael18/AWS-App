{
  "name": "skillswap-backend",
  "version": "1.0.0",
  "description": "Backend API for SkillSwap application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:cognito": "BYPASS_AUTH=false nodemon server.js",
    "dev:bypass": "BYPASS_AUTH=true nodemon server.js",
    "test": "jest",
    "build": "echo 'No build step needed for Node.js'",
    "lint": "eslint .",
    "setup-db": "node src/database/setup.js setup",
    "cleanup-db": "node src/database/setup.js cleanup",
    "setup-db-v3": "node -e \"require('./src/database/setup-v3').setupTables()\"",
    "migrate-to-dynamodb": "node scripts/migrate-to-dynamodb.js",
    "verify-migration": "node scripts/migrate-to-dynamodb.js --verify",
    "setup-s3": "node scripts/setup-s3.js",
    "verify-s3": "node scripts/setup-s3.js --verify",
    "s3-demo-config": "node scripts/setup-s3.js --demo-config",
    "test-cognito": "node scripts/test-cognito.js"
  },
  "keywords": [
    "aws",
    "nodejs",
    "express",
    "skillswap",
    "dynamodb",
    "cognito"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.844.0",
    "@aws-sdk/client-dynamodb": "^3.844.0",
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/lib-dynamodb": "^3.844.0",
    "@aws-sdk/s3-request-presigner": "^3.844.0",
    "@codegenie/serverless-express": "^4.17.0",
    "aws-sdk": "^2.1692.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "mongodb": "^6.17.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "serverless": "^4.4.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.8.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
